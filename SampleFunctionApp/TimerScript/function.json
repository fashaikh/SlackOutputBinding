{
  "disabled": false,
  "bindings": [
    {
      "name": "myTimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "*/5 * * * * *"
    },
    {
      "name": "inputBlob",
      "type": "blobTrigger",
      "direction": "inout",
      "path": "incoming/{startutcdatetime}|{sendminutesinterval}|{splitinto}|{template}|{version}|{environment}|{username}/{filename}.csv",
      "connection": "AzureWebJobsDashboard"
    }
  ]
}